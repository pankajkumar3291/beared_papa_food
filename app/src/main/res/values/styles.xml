<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="ThemeOverlay.MySwitch" parent="">
        <item name="android:colorControlActivated">@color/coloryellow</item>
        <item name="android:colorForeground">@color/colorPrimary</item>
        <item name="android:colorControlNormal">@color/colorPrimary</item>

    </style>

    <style name="progressDialogStyle" parent="Theme.AppCompat.Light">s
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

        <!-- You can also set colorControlNormal, colorControlActivated
             colorControlHighlight, and colorSwitchThumbNormal. -->



</resources>
